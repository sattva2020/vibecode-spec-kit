{
  "name": "Memory Bank Monitoring Dashboard",
  "description": "Comprehensive monitoring dashboard for Memory Bank system",
  "panels": [
    {
      "id": 1,
      "title": "System Overview",
      "type": "row",
      "collapsed": false
    },
    {
      "id": 2,
      "title": "CPU Usage",
      "type": "graph",
      "targets": [
        {
          "expr": "memory_bank_cpu_usage_percent",
          "legendFormat": "CPU Usage %"
        }
      ],
      "yAxes": [
        {
          "min": 0,
          "max": 100,
          "unit": "percent"
        }
      ]
    },
    {
      "id": 3,
      "title": "Memory Usage",
      "type": "graph",
      "targets": [
        {
          "expr": "memory_bank_memory_usage_percent",
          "legendFormat": "Memory Usage %"
        }
      ],
      "yAxes": [
        {
          "min": 0,
          "max": 100,
          "unit": "percent"
        }
      ]
    },
    {
      "id": 4,
      "title": "Request Rate",
      "type": "graph",
      "targets": [
        {
          "expr": "rate(memory_bank_requests_total[5m])",
          "legendFormat": "Requests/sec"
        }
      ]
    },
    {
      "id": 5,
      "title": "Error Rate",
      "type": "graph",
      "targets": [
        {
          "expr": "rate(memory_bank_errors_total[5m])",
          "legendFormat": "Errors/sec"
        }
      ]
    },
    {
      "id": 6,
      "title": "Response Time",
      "type": "graph",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(memory_bank_response_time_seconds_bucket[5m]))",
          "legendFormat": "95th percentile"
        },
        {
          "expr": "histogram_quantile(0.50, rate(memory_bank_response_time_seconds_bucket[5m]))",
          "legendFormat": "50th percentile"
        }
      ]
    },
    {
      "id": 7,
      "title": "Active Users",
      "type": "singlestat",
      "targets": [
        {
          "expr": "memory_bank_active_users",
          "legendFormat": "Active Users"
        }
      ],
      "valueName": "current"
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "refresh": "5s",
  "timezone": "browser",
  "annotations": {
    "list": [
      {
        "name": "Alerts",
        "datasource": "prometheus",
        "enable": true,
        "expr": "ALERTS{alertstate=\"firing\"}"
      }
    ]
  }
}
