# RAG-Powered Code Assistant Environment Variables
# ================================================

# AI Router Configuration
AI_ROUTER_ENABLED=true
AI_DEFAULT_PROVIDER=copilot
AI_FALLBACK_PROVIDER=claude
AI_PROVIDER_MODE=subscription_first

# GitHub Copilot Integration (ВАША ПЛАТНАЯ ПОДПИСКА!)
# Получите API ключ на: https://github.com/settings/tokens
GITHUB_COPILOT_API_KEY=your-github-copilot-api-key-here
GITHUB_COPILOT_API_URL=https://api.githubcopilot.com/v1
GITHUB_COPILOT_SESSION_NAME=n8n-ai-router

# Cursor IDE Integration (ВАША ПЛАТНАЯ ПОДПИСКА!)
# Получите API ключ в настройках Cursor IDE
CURSOR_API_KEY=your-cursor-api-key-here
CURSOR_API_URL=https://api.cursor.sh/v1
CURSOR_SESSION_NAME=n8n-ai-router

# Anthropic Claude (если есть API ключ)
ANTHROPIC_API_KEY=your-anthropic-api-key-here
CLAUDE_MODEL=claude-3-5-sonnet-20241022

# OpenAI (если есть API ключ)
OPENAI_API_KEY=your-openai-api-key-here
OPENAI_MODEL=gpt-4-turbo-preview

# Ollama Configuration (локальный)
OLLAMA_URL=http://ollama:11434
OLLAMA_GRAPH_MODEL=nomic-embed-text
OLLAMA_INDEX_MODEL=qwen2.5-coder:1.5b

# Supabase Configuration
SUPABASE_URL=http://supabase-db:5432
SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# n8n Configuration
N8N_HOST=localhost
N8N_PORT=5678
N8N_PROTOCOL=http
N8N_BASIC_AUTH_ACTIVE=false

# Performance Settings
REQUEST_TIMEOUT=60
MAX_CONCURRENT_REQUESTS=10
LOG_LEVEL=INFO
LOG_FORMAT=json
