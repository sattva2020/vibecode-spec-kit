# ===========================================
# RAG SYSTEM ENVIRONMENT CONFIGURATION
# ===========================================

# Database Configuration
POSTGRES_DB=rag_system
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_HOST=localhost
POSTGRES_PORT=5432

# Supabase Configuration
SUPABASE_URL=http://localhost:8000
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here
JWT_SECRET=your_jwt_secret_key_here

# AI Services Configuration
# Ollama (только для графа БД и embeddings)
OLLAMA_URL=http://localhost:11434
OLLAMA_GRAPH_MODEL=nomic-embed-text
OLLAMA_INDEX_MODEL=qwen2.5-coder:1.5b

# AI Router Configuration
AI_ROUTER_ENABLED=true
AI_DEFAULT_PROVIDER=cursor
AI_FALLBACK_PROVIDER=claude
AI_PROVIDER_MODE=subscription_first  # subscription_first, paid_only, cursor_only, copilot_only

# Cursor IDE Integration (ВАШИ ПЛАТНЫЕ ПОДПИСКИ!)
CURSOR_API_KEY=your-cursor-api-key
CURSOR_API_URL=https://api.cursor.sh/v1
CURSOR_SESSION_NAME=n8n-ai-router

# GitHub Copilot Integration (ВАША ПЛАТНАЯ ПОДПИСКА!)
GITHUB_COPILOT_API_KEY=your-github-copilot-api-key
GITHUB_COPILOT_API_URL=https://api.githubcopilot.com/v1
GITHUB_COPILOT_SESSION_NAME=n8n-ai-router

# Anthropic Claude (основной для n8n и анализа)
ANTHROPIC_API_KEY=your-claude-api-key
CLAUDE_MODEL=claude-3-5-sonnet-20241022
CLAUDE_MAX_TOKENS=8192
CLAUDE_TEMPERATURE=0.1

# OpenAI (для генерации кода и быстрых задач)
OPENAI_API_KEY=your-openai-api-key
OPENAI_MODEL=gpt-4-turbo-preview
OPENAI_CODE_MODEL=gpt-4-code-interpreter
OPENAI_MAX_TOKENS=4096
OPENAI_TEMPERATURE=0.2

# Google Gemini (альтернатива)
GOOGLE_API_KEY=your-google-api-key
GOOGLE_MODEL=gemini-pro
GOOGLE_MAX_TOKENS=2048

LIGHTRAG_URL=http://localhost:8001
LIGHTRAG_API_KEY=your_lightrag_api_key_here

# n8n Configuration
N8N_URL=http://localhost:5678
N8N_USER=admin
N8N_PASSWORD=your_n8n_password_here
N8N_ENCRYPTION_KEY=your_n8n_encryption_key_here
N8N_WEBHOOK_URL=http://localhost:5678/

# Intelligent N8N System Configuration
INTELLIGENT_N8N_URL=http://localhost:8002
INTELLIGENT_N8N_API_KEY=your_api_key_here

# RAG Proxy Configuration
RAG_PROXY_URL=http://localhost:8080
RAG_PROXY_HOST=0.0.0.0
RAG_PROXY_PORT=8080

# Monitoring Configuration
PROMETHEUS_URL=http://localhost:9090
GRAFANA_URL=http://localhost:3000
GRAFANA_USER=admin
GRAFANA_PASSWORD=your_grafana_password_here

# Logging Configuration
LOG_LEVEL=INFO
LOG_FORMAT=json
LOG_FILE_PATH=./logs

# Performance Configuration
MAX_CONCURRENT_REQUESTS=100
REQUEST_TIMEOUT=30000
CACHE_TTL=3600
MEMORY_LIMIT=2G

# Security Configuration
API_RATE_LIMIT=1000
CORS_ORIGINS=*
ENABLE_AUTH=true
SESSION_SECRET=your_session_secret_here

# Development Configuration
DEBUG=false
ENVIRONMENT=development
HOT_RELOAD=true

# Windows Specific Configuration
WINDOWS_DOCKER_MODE=true
POWERSHELL_EXECUTION_POLICY=RemoteSigned
